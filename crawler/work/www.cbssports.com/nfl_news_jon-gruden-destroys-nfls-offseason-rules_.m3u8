</head>
<body>

<script>
$(function() {

    var video = $('.flexVideo.lite .video').get(0);

    if (Hls.isSupported()) {
        var hls = new Hls();
        hls.loadSource(decodeURIComponent('https://cbssportsdc-i.akamaihd.net/hls/live/518856/dc_1/master-250.m3u8
'));
        hls.attachMedia(video);
        hls.on(Hls.Events.MANIFEST_PARSED,function() {
            video.play();
        });
    }
    else if (video.canPlayType('application/vnd.apple.mpegurl')) {
        video.src = 'https://cbssportsdc-i.akamaihd.net/hls/live/518856/dc_1/master-250.m3u8
';
        video.addEventListener('canplay',function() {
            video.play();
        });
    }
});
</script>

<style>
body {
    background-color: transparent !important;
}
.flexVideo {
    width: 100%;
    height: 0;
    display: block;
    position: relative;
    padding-bottom: 56.25%;
}

.flexVideo .video {
    pointer-events: none;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
}
</style>

<div class="flexVideo lite">
    <video class="video" muted playsinline autoplay></video>
